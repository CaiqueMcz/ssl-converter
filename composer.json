{
  "name": "caiquemcz/ssl-converter",
  "description": "SSL certificate converter (PEM/DER/PKCS12), CA bundle and private keys in PHP.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SslConverter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SslConverter\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "doctrine/collections": "^1.8",
    "symfony/process": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^4.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "lint": "phpcs --standard=PSR12 src",
    "lint-fix": "phpcbf --standard=PSR12 src"
  }
}